{
	"semi": true,
	"singleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"printWidth": 80,
	"endOfLine": "lf",
	"arrowParens": "always",
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"plugins": [
		"prettier-plugin-tailwindcss",
		"prettier-plugin-organize-imports"
	],
	"htmlWhitespaceSensitivity": "ignore",
	"vueIndentScriptAndStyle": true
}
